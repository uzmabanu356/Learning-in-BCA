#include<iostream>
using namespace std;

int main()
{
    int a,b,sum,sub,multi,div,ram;
    cout<<"Enter two number:"<<endl;
    cin>>a>>b;
    sum=a+b;
    cout<<"sum="<<sum<<endl;
    sub=a-b;
    cout<<"sub="<<sub<<endl;
    multi=a*b;
    cout<<"multiple="<<multi<<endl;
    div=a/b;
    cout<<"division="<<div<<endl;
    ram=a%b;
    cout<<"ramainder="<<ram<<endl;
    return 0;
}